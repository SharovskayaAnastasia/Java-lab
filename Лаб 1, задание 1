import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Введите число x: ");
        int x = scanner.nextInt();
        System.out.print("Введите число y: ");
        int y = scanner.nextInt();

        int b;
        int s;

        if (x > y) {
            b = x;
            s = y;
        } else {
            b = y;
            s = x;
        }

        int resultBigger = b * 2;
        double resultSmaller = s / 3.0;

        System.out.println("Большее число: " + b);
        System.out.println("Большее число * 2 = " + resultBigger);
        System.out.println("Меньшее число: " + s);
        System.out.println("Меньшее число / 3 = " + resultSmaller);
        scanner.close();
    }
}
