public class Intern extends Employee {

    public Intern(String name, double salary) {
        super(name, salary);
    }

    public void study() {
        System.out.println("Стажёр " + getName() + " учится новым навыкам.");
    }

    @Override
    public String toString() {
        return "Intern{" + super.toString() + "}";
    }
}
